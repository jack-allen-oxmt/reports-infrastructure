{
  "index_templates": [
    {
      "name": "request-events-template",
      "index_template": {
        "index_patterns": [
          "request-events-*"
        ],
        "template": {
          "mappings": {
            "properties": {
              "path": {
                "type": "keyword"
              },
              "assetTypes": {
                "type": "keyword"
              },
              "status_code": {
                "type": "integer"
              },
              "siteTokens": {
                "type": "keyword"
              },
              "method": {
                "type": "keyword"
              },
              "module": {
                "type": "keyword"
              },
              "session_id": {
                "type": "keyword"
              },
              "response_time": {
                "type": "integer"
              },
              "user": {
                "type": "keyword"
              },
              "timestamp": {
                "format": "epoch_millis",
                "type": "date"
              }
            }
          }
        },
        "composed_of": [],
        "priority": 500
      }
    }
  ]
}